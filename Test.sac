content type Text is recognized by css "p"
content type Image is recognized by css "img"

content type FaqItem
	is recognized by css "div.faqItem"
	recognize
		question as Text by css "h4"
		answer as Text

content type Gallery
	is recognized by css "div.gallery"
	recognize
		name as Text by css "h4"
		images as GalleryImage

content type GalleryImage
	is recognized by css "div.image"
	recognize
		caption as Text by css "span.caption"
		image as Image

page type Service
	is recognized by url pattern "\\/service$"
	recognize
		faqItems as FaqItem
		imageGallery as Gallery

